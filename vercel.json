{
    "builds": [
        {
            "src": "fotoawards/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "excludeFiles": "**",
                "includeFiles": "fotoawards/**",
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "fotoawards/wsgi.py"
        }
    ]
}